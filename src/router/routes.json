{
  "/": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Dashboard requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login",
      "excludeRedirectQuery": false
    }
  },
  "/staff": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Staff page requires authentication",
            "rule": "authenticated"
          }
        ]
      },
      "redirectOnDeny": "/login",
      "excludeRedirectQuery": false
    }
  },
  "/login": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Login page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/signup": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Signup page is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/reset-password": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reset password is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/reset-password/**/*": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Reset password paths are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/prompt-password": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Prompt password is public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/prompt-password/**/*": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Prompt password paths are public",
            "rule": "public"
          }
        ]
      }
    }
  },
  "/callback": {
    "allow": {
      "when": {
        "conditions": [
          {
            "label": "Callback page is public",
            "rule": "public"
          }
        ]
      }
    }
  }
}